<?xml version="1.0" encoding="UTF-8"?>

<schema 

xmlns="http://www.w3.org/2001/XMLSchema" 
targetNamespace="http://org.example/createServiceRequest" 
xmlns:req="http://org.example/createServiceRequest" 
xmlns:tns="http://org.example/service"
elementFormDefault="qualified">

  <import namespace="http://org.example/service" schemaLocation="Service.xsd"/>

<element name="CreateServiceRequest"
	     type="req:CreateServiceRequest"/>

<complexType name="CreateServiceRequest">

		<sequence>
        <element name="serviceID" type="tns:rServiceID"/>
        <element name="docName" type="tns:rDocName"/>
        <element name="serviceName" type="tns:rServiceName"/>
        <element name="docSFreeTime" type="tns:rDocsFreeTime"/>
        </sequence>
        
</complexType>

</schema>